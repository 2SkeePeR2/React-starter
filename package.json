{
    "name": "flipr",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@material-ui/core": "^4.11.3",
        "@material-ui/icons": "^4.11.2",
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react": "^11.2.6",
        "@testing-library/user-event": "^12.8.3",
        "@types/jest": "^26.0.22",
        "@types/node": "^12.20.10",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "@types/react-redux": "^7.1.16",
        "axios": "^0.21.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.3",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "redux-devtools-extension": "^2.13.9",
        "redux-thunk": "^2.3.0",
        "typescript": "^4.2.4",
        "web-vitals": "^1.1.1"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "yarn run pretty"
        ],
        "*.{js,css,md}": "prettier --write"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "pretty": "prettier --write \"src/{api, components, store, constants, model, pages, store, styles}/**.{ts,js,tsx,jsx}\" \"src/{App,index}.{tsx,css}\" ",
        "eject": "react-scripts eject",
        "prepare": "husky install"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "husky": "^6.0.0",
        "lint-staged": "^10.5.4",
        "prettier": "^2.2.1"
    }
}
